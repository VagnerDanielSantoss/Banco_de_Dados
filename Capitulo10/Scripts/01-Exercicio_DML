SELECT CLIENTE.NOME, CLIENTE.EMAIL, TELEFONE.DDD, TELEFONE.NUMERO AS CELULAR
FROM CLIENTE
    INNER JOIN ENDERECO 
    ON CLIENTE.IDCLIENTE = ENDERECO.ID_CLIENTE
    INNER JOIN TELEFONE 
    ON CLIENTE.IDCLIENTE = TELEFONE.ID_CLIENTE
WHERE
SEXO = "F"
AND
    ENDERECO.ESTADO = "SP" AND TELEFONE.TIPO = "CELULAR"
AND 
    EMAIL IS NOT NULL;
