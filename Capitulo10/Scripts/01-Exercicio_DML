SELECT CLIENTE.IDCLIENTE, CLIENTE.NOME, CLIENTE.EMAIL, CLIENTE.SEXO, ENDERECO.CIDADE, ENDERECO.BAIRRO, TELEFONE.TIPO
FROM CLIENTE
    INNER JOIN ENDERECO 
    ON CLIENTE.IDCLIENTE = ENDERECO.ID_CLIENTE
    INNER JOIN TELEFONE 
    ON CLIENTE.IDCLIENTE = TELEFONE.ID_CLIENTE
WHERE 
SEXO = "F"
AND 
BAIRRO = "FLORES LINDAS" AND CIDADE = "CURITIBA";
