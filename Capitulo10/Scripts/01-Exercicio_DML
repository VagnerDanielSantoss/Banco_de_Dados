                 /* RELATORIO DE HOMENS */

SELECT CLIENTE.IDCLIENTE, CLIENTE.NOME, CLIENTE.CPF, CLIENTE.EMAIL, CLIENTE.SEXO,
       ENDERECO.RUA, ENDERECO.BAIRRO, ENDERECO.CIDADE, ENDERECO.ESTADO,
       TELEFONE.DDD, TELEFONE.NUMERO, TELEFONE.TIPO
FROM CLIENTE
    INNER JOIN ENDERECO 
    ON CLIENTE.IDCLIENTE = ENDERECO.ID_CLIENTE
    INNER JOIN TELEFONE 
    ON CLIENTE.IDCLIENTE = TELEFONE.ID_CLIENTE
WHERE SEXO = "M";

